        "    },\r\n",
        "    _onGetCityCB : function (data) {\r\n",
        "        var suffix = data.suffix;\r\n",
        "        $(\"#hsnCd\" + suffix).html(data.hsnOption);\r\n",
        "        if (data.townOption) {\r\n",
        "             $(\"#townCd\" + suffix).html(data.townOption);\r\n",
        "        }\r\n",
        "        $(\"#hsnCd\" + suffix).bind(\"change\", function () {ContactAddr._onCityChange(this);});\r\n",
        "        $(\"#townCd\" + suffix).bind(\"change\", function () {ContactAddr._onTownChange(this);});\r\n",
        "    },\r\n",
        "    _onCityChange : function (elm) {\r\n",
        "        var suffix = elm.id.replace(\"hsnCd\", \"\");\r\n",
        "        var val = $(elm).val();\r\n",
        "        var hsnNm = (val == \"\") ? \"\" : $(elm).find(\"> option:selected\").text();\r\n",
        "        $(\"#hsnNm\" + suffix).val(hsnNm);\r\n",
        "        $(\"#zipCd\" + suffix).val(\"\");\r\n",
        "        $(\"#townCd\" + suffix).html(\"<option value=\\\"\\\">\u8acb\u9078\u64c7</option>\");\r\n",
        "        $(\"#townNm\" + suffix).val(\"\");\r\n",
        "        if (val == \"\") {\r\n",
        "            return;\r\n",
        "        }\r\n",
        "        var url = this._URL;\r\n",
        "        var param = {\r\n",
        "            fn : 2,\r\n",
        "            hsnCd : val,\r\n",
        "            suffix : suffix\r\n",
        "        };\r\n",
        "        ajaxPost(url, param, this._onCityChangeCB);\r\n",
        "    },\r\n",
        "    _onCityChangeCB : function (data) {\r\n",
        "        var suffix = data.suffix;\r\n",
        "        $(\"#townCd\" + suffix).html(data.townOption);\r\n",
        "    },\r\n",
        "    _onTownChange : function (elm) {\r\n",
        "        var suffix = elm.id.replace(\"townCd\", \"\");\r\n",
        "        var val = $(elm).val();\r\n",
        "        var townNm = (val == \"\") ? \"\" : $(elm).find(\"> option:selected\").text();\r\n",
        "        $(\"#townNm\" + suffix).val(townNm);\r\n",
        "        var zipCd = $(elm).find(\"> option:selected\").attr(\"zipCd\");\r\n",
        "        $(\"#zipCd\" + suffix).val(zipCd);\r\n",
        "    },\r\n",
        "    _reset : function () {\r\n",
        "        var suffix = this._suffix;\r\n",
        "        $(\"#hsnCd\" + suffix).html(\"<option value=\\\"\\\">\u8acb\u9078\u64c7</option>\");\r\n",
